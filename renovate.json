{
  "extends": [
    "config:js-lib",
    ":automergeMinor",
    ":automergePr",
    "algolia"
  ],
  "rebaseStalePRs": false,
  "ignorePresets": [
    ":ignoreModulesAndTests"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "^@types"
      ],
      "depTypeList": [
        "devDependencies"
      ],
      "rangeStrategy": "replace"
    },
    {
      "paths": [
        "examples/react-router-v3"
      ],
      "ignoreDeps": [
        "react-router"
      ]
    }
  ]
}
