{
  "name": "instantsearch.js",
  "version": "0.0.1",
  "description": "Instant search kit for Algolia search",
  "main": "index.js",
  "author": "Algolia <support@algolia.com>",
  "scripts": {
    "lint": "./scripts/lint.sh",
    "build": "./scripts/build.sh",
    "dev": "./scripts/dev.sh",
    "checkstyle": "jsfmt --diff",
    "jsfmt": "jsfmt",
    "test": "babel-node test/*.js && npm run lint",
    "test:watch": "nodemon -i node_modules/ -i coverage/ -i .git/ -i dist/ -q --exec 'babel-node test/*.js | tap-spec'",
    "test:coverage": "babel-node node_modules/.bin/isparta cover test/*.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "browser": {
    "lodash": "lodash-compat"
  },
  "devDependencies": {
    "@algolia/jsfmt": "1.3.0",
    "babel": "5.8.19",
    "babel-eslint": "4.0.5",
    "babel-loader": "5.3.2",
    "eslint": "0.24.1",
    "eslint-config-airbnb": "0.0.7",
    "eslint-config-algolia": "2.1.2",
    "eslint-plugin-react": "3.1.0",
    "isparta": "3.0.3",
    "nodemon": "1.4.0",
    "onchange": "1.1.0",
    "pretty-bytes": "2.0.1",
    "proxyquire": "1.6.0",
    "raw-loader": "0.5.1",
    "sinon": "1.15.4",
    "tap-spec": "4.0.2",
    "tape": "4.0.1",
    "uglifyjs": "2.4.10",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.10.1"
  },
  "dependencies": {
    "algoliasearch": "3.7.5",
    "algoliasearch-helper": "2.2.0",
    "classnames": "2.1.3",
    "hogan.js": "3.0.2",
    "lodash": "3.10.0",
    "react": "0.13.3"
  },
  "license": "MIT"
}
