{
  "name": "instantsearch.js",
  "version": "0.0.1",
  "description": "Instant search kit for Algolia search",
  "main": "index.js",
  "author": "Algolia <support@algolia.com>",
  "scripts": {
    "lint": "./scripts/lint.sh",
    "build": "./scripts/build.sh",
    "dev": "./scripts/dev.sh",
    "test": "./scripts/test.sh && npm run lint",
    "test:watch": "nodemon -i node_modules/ -i coverage/ -i .git/ -i dist/ -q --exec './scripts/test.sh | tap-spec'",
    "test:coverage": "babel-node node_modules/.bin/isparta cover ./scripts/test.sh",
    "doctoc": "doctoc --maxlevel 3 README.md"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "browser": {
    "lodash": "lodash-compat"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-eslint": "4.1.1",
    "babel-loader": "5.3.2",
    "babel-tape-runner": "1.2.0",
    "css-loader": "0.16.0",
    "doctoc": "0.15.0",
    "eslint": "1.4.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-config-algolia": "3.0.0",
    "eslint-plugin-react": "3.3.2",
    "isparta": "3.0.4",
    "nodemon": "1.4.1",
    "onchange": "2.0.0",
    "pretty-bytes": "2.0.1",
    "proxyquire": "1.7.1",
    "sinon": "1.16.1",
    "style-loader": "0.12.3",
    "tap-spec": "4.1.0",
    "tape": "4.2.0",
    "uglifyjs": "2.4.10",
    "webpack": "1.12.0",
    "webpack-dev-server": "1.10.1"
  },
  "dependencies": {
    "algoliasearch": "3.7.8",
    "algoliasearch-helper": "2.3.0",
    "classnames": "2.1.3",
    "hogan.js": "3.0.2",
    "lodash": "3.10.1",
    "raw-loader": "0.5.1",
    "react": "0.13.3",
    "to-factory": "1.0.0"
  },
  "license": "MIT"
}
