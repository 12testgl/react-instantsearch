{
  "name": "instantsearch.js",
  "version": "1.2.4",
  "description": "instantsearch.js is a library of widgets to build high performance instant search experiences using Algolia",
  "main": "dist-es5-module/index.js",
  "author": "Algolia <support@algolia.com>",
  "scripts": {
    "build": "./scripts/build.sh",
    "dev": "./scripts/dev.sh",
    "dev:docs": "./scripts/dev-docs.sh",
    "doctoc": "doctoc --maxlevel 3 README.md CONTRIBUTING.md",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint . --quiet --no-color",
    "lint:css": "sass-lint --verbose",
    "docs:jsdoc": "babel-node ./scripts/docs/gen-widget-doc.js",
    "docs:update-website": "./scripts/docs/update-website.sh",
    "docs:update-website-search-index": "./scripts/docs/update-website-search-index.sh",
    "finish-release": "./scripts/finish-release.sh",
    "view-changelog": "conventional-changelog -p angular",
    "release": "./scripts/release.sh",
    "test": "NODE_ENV=production BABEL_ENV=test mocha --opts scripts/mocha.opts --compilers js:babel/register --reporter dot && npm run lint",
    "test:browser": "NODE_ENV=production BABEL_ENV=test karma start scripts/karma.conf.js",
    "test:coverage": "BABEL_ENV=test babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha -- --opts scripts/mocha.opts --reporter dot && npm run lint",
    "test:watch": "NODE_ENV=production BABEL_ENV=test mocha --opts scripts/mocha.opts --compilers js:babel/register --reporter min --watch",
    "test:browser:watch": "NODE_ENV=production BABEL_ENV=test karma start scripts/karma.conf.js",
    "test:functional": "wdio functional-tests/wdio.conf.js",
    "test:functional:dev": "babel-node scripts/dev-functional-tests",
    "test:functional:dev:debug-server": "babel-node scripts/dev-functional-tests-debug-server",
    "shrinkwrap": "npm-shrinkwrap --dev"
  },
  "repository": "algolia/instantsearch.js",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-rewire": "^0.1.22",
    "clean-css": "^3.4.9",
    "collect-json": "^1.0.5",
    "compression": "^1.6.1",
    "conventional-changelog": "^0.5.3",
    "dmd": "^1.3.4",
    "doctoc": "^1.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-config-algolia": "^4.5.0",
    "eslint-plugin-algolia": "^1.5.0",
    "eslint-plugin-react": "^3.15.0",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "expose-loader": "^0.7.1",
    "express": "^4.13.4",
    "gh-pages": "^0.9.0",
    "jsdoc-to-markdown": "^1.3.3",
    "jsdom": "^8.0.2",
    "jsdom-global": "^1.6.1",
    "json": "^9.0.3",
    "karma": "^0.13.18",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-sauce-launcher": "^0.3.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "mversion": "^1.10.1",
    "node-sass": "^3.4.2",
    "npm-shrinkwrap": "^200.4.0",
    "postcss-cli": "^2.5.0",
    "pretty-bytes": "^2.0.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.5.0",
    "sass-lint": "^1.5.0",
    "semver": "^5.1.0",
    "sinon": "^2.0.0-pre",
    "uglify-js": "^2.6.1",
    "watch": "^0.17.1",
    "webdriverio-algolia-fork-launcher": "^3.4.2",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "algoliasearch": "^3.11.0",
    "algoliasearch-helper": "^2.9.0",
    "classnames": "^2.2.3",
    "events": "^1.1.0",
    "hogan.js": "^3.0.2",
    "lodash": "^3.10.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-nouislider": "^1.7.0",
    "to-factory": "^1.0.0"
  },
  "license": "MIT"
}
